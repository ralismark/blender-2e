	"logging": {
		"version": 1,
		"disable_existing_loggers": false,
		"formatters": {
			"standard": {
				"format": "%(levelname)-8s %(name)-15s: %(message)s",
				"datefmt": "%d/%m %H:%M:%S"
			}
		},
		"handlers": {
			"default": {
